: space-between;\n\n    > * {\n      margin: 0 $element-margin;\n    }\n\n    > *:first-child {\n      margin-left: 0;\n    }\n\n    > *:last-child {\n      margin-right: 0;\n    }\n  }\n}\n","sass/components/_card.scss":".card {\n  padding: $element-margin;\n  min-width: 360px;\n  background: white;\n  box-shadow: 0 3px 6px rgba(0, 0, 0, 0.1);\n}\n","sass/components/_button.scss":".button {\n  margin: 0;\n  padding: 0 $element-margin;\n  height: 32px;\n  font-size: 12px;\n  text-transform: capitalize;\n  background: transparent;\n  border: none;\n  border-radius: 0.2rem;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  cursor: pointer;\n\n  transition: all 150ms ease;\n\n  &:hover {\n    background: rgba(0, 0, 0, 0.1);\n  }\n\n  &.primary {\n    background: $accent;\n    @if (lightness($accent) > 50) {\n      color: black; // Lighter backgorund, return dark color\n    } @else {\n      color: white; // Darker background, return light color\n    }\n\n    &:hover {\n      background: lighten($accent, 10%);\n    }\n  }\n\n  &.raised {\n    box-shadow: 0 3px 6px rgba(0, 0, 0, 0.1);\n\n    &:hover {\n      box-shadow: 0 5px 8px rgba(0, 0, 0, 0.15);\n    }\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"1d62af63507504237d7b48b110548f9f","cacheData":{"env":{}}}